<%= form_for([@room, @room.reviews.new]) do |f| %>
 
        <div class="form-group">
 
      <%= f.text_area :comment, rows: 3, class: "form-control" %>               
 
     </div>
 
        <div class="actions">
 
     <%= f.submit "Evaluez", class: "btn btn-primary" %>
 
 </div>
 
 <% end %>

<script>
 
      $('#user_stars').raty({
 
      path: '/assets',
 
      scoreName: 'review[star]', 
 
      score: 1
 
      });
 
 </script>